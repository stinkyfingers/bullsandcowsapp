{
  "expo": {
    "name": "bullsandcowsapp",
    "slug": "bullsandcows",
    "owner": "airfox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/BullsAndCows.drawio-1.png",
    "splash": {
      "image": "./assets/BullsAndCows.drawio-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.airfox.bullsandcows"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/BullsAndCows.drawio-1.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
